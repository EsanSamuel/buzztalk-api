{
    "version": 2,
    "framework": "node",
    "buildCommand": "cd server && npm install && npm run build"
    "build": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
